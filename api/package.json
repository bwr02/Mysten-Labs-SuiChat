{
  "name": "backend",
  "type": "module",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "dev": "concurrently \"pnpm api:dev\" \"pnpm indexer\"",
    "db:setup:dev": "npx prisma migrate dev --name init",
    "db:reset:dev": "npx prisma db push --force-reset",
    "db:studio": "npx prisma studio",
    "api:dev": "npx tsx server.ts",
    "indexer": "npx tsx indexer.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.1",
    "@types/ws": "^8.5.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@mysten/sui": "^1.21.2",
    "@prisma/client": "5.21.1",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "prisma": "^5.21.1",
    "ws": "^8.18.0"
  }
}